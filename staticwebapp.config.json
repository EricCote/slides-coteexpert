//This file is only used when deployed on Azure Static Web Apps.

{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/img/*.{png,jpg,gif}", "/css/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none' ; img-src *; connect-src 'self' data:"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".gltf": "model/gltf+json"
  }
}
